  <script src="./js/common.js" charset="utf-8"></script>
  <script>
  	function editServeName(t) {
  		var td = $($(t).parents('td')[0]);
  		td.children('.exchange-content').css('display', 'inline-block')
  		td.children('span').css('display', 'none');
  		td.children('a.col-edit').css('display', 'none');
  		td.children('div.col-edit').css('display', 'inline-block');
  	}
  	function changeServeName(t,e) {
  		e.stopPropagation();
  		var p = $(t).parent('');
  		p.css('display', 'none');
  		var prev = p.prev('');
  		var serveName = prev.prev('').prev('').val();
  		// 显示span内容，隐藏编辑框
  		prev.prev('').children('a').text(serveName)
  		prev.css('display', 'inline-block').prev('').css('display', 'inline-block').prev('').css('display', 'none');
  		// $.post('#', {param1: serveName}, function(data, textStatus, xhr) {
  		// });
  	}
  	function cancelOperate(t,e) {
  		e.stopPropagation();
  		var p = $(t).parent('');
  		p.css('display', 'none');
  		var prev = p.prev('');
  		// 隐藏编辑框，复原编辑框中内容
  		prev.prev('').prev('').val(prev.prev('').children('a').text());
  		prev.css('display', 'inline-block').prev('').css('display', 'inline-block');
  		prev.prev('').prev('').css('display', 'none');
  	}
  	function moveToTop(t) {
  		$('tbody tr').insertAfter($(t).parents('tr'));
  	}

  	$("#selectall").click(function(e){
  		if($(this).prop('checked'))
			$('.check-serve').prop('checked', true);
		else
			$('.check-serve').prop('checked', false);

  	});
  </script>
