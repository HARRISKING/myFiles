body {
	background-color: #fff;
	width: 100%;
}

.car {
	padding: 0 10px;
	background-color: #fff;
	font-size: 15px;
}

.car ul li {
	border-bottom: 1px solid #eee;
	line-height: 50px;
}

.car ul li p {
	line-height: 30px;
}

.car ul li:not(:last-child) {
	display: flex;
	justify-content: space-between;
	align-items: center;
}


.car .word{
	width: 85%;
	overflow: hidden;
	text-overflow: ellipsis;
	white-space: nowrap;
}

.pic{
	/*display: flex;
	justify-content: space-around;*/
	overflow-x: hidden;
	white-space: nowrap;
	width: 100%;

}

.pic img{
	width: 24.3%;
}
