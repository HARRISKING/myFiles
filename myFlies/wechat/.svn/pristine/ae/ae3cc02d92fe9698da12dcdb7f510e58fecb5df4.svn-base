div#imgdiv {
    position: absolute;
    max-width: 500px;
    height: auto;
    z-index: -1;
    background-color: rgb(5, 5, 5);
}

div#imgdiv > img {
    width: 100%;
    height: 100%;
    min-height: 69rem;
    position: fixed;
}

div.cartest {
    width: 100%;
    background: #dfe8e9;
}

div.cartest img:nth-child(1) {
    width: 4%;
    float: left;
    padding-left: 7%;
    padding-top: 6%;
}

div.cartest img:nth-child(2) {
    width: 23%;
    margin-left: 26%;
    margin-top: 5%;
    margin-bottom: 1%;
}

span.arrtop {
    background: url(../img/check/arrtop.png) no-repeat 0 0;
    display: block;
    width: 6%;
    height: 30%;
    float: left;
    background-size: contain;
    margin-left: 5%;
    margin-top: 6%;
}

span.caricon {
    background: url(../img/check/cartest.png) no-repeat 0 16px;
    display: block;
    width: 26%;
    height: 80%;
    background-size: contain;
    padding-top: 16%;
    margin-left: auto;
    margin-right: auto;
}

div.watch {
    width: 80%;
    height: 45%;
    margin-left: auto;
    margin-right: auto;
    position: relative;
}

div.watch span {
    color: #fff;
    font-size: 0.8rem;
    position: absolute;
    font-family: 微软雅黑;
}

div.watch img {
    width: 100%;
    position: absolute;
}

div.pointer {
    background: url(../img/check/pointer2.png) no-repeat 0 0;
    width: 0.75rem;
    height: 3.75rem;
    position: absolute;
    background-size: contain;
}


